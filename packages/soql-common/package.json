{
  "name": "@salesforce/soql-common",
  "version": "0.2.1",
  "description": "SOQL Common utility library",
  "engines": {
    "node": "*"
  },
  "main": "lib/",
  "scripts": {
    "compile": "tsc --project .",
    "include-soql-parser": "run-script-os",
    "include-soql-parser:default": "node /scripts/include-soql-parser.js",
    "include-soql-parser:windows": "node .\\scripts\\include-soql-parser.js",
    "build": "yarn run compile && yarn run include-soql-parser",
    "clean": "rimraf lib && rimraf node_modules",
    "test": "jest --runInBand",
    "test:unit:coverage": "jest --runInBand --coverage"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "jest": "26.1.0",
    "run-script-os": "^1.1.5",
    "shelljs": "^0.8.4",
    "ts-jest": "26.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forcedotcom/soql-common.git"
  },
  "keywords": [
    "soql",
    "common"
  ],
  "author": "Salesforce",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org/"
  },
  "license": "BSD-3-Clause"
}
